if(grepl("\\^LO$",x)){w=which(expr[,m]<triS[1,m])&expr[,m]>triS[2,m]}
if(grepl("\\^HI$",x)){w=which(expr[,m]>triS[1,m])}
return(w)
})
if(length(CL_label2[[i]])>1){rows=Reduce(intersect,rows)}
CL[[i]]=rows
}
